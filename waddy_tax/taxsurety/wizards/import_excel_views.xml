<?xml version="1.0"?>
<odoo>
    <record id="partner_matching_wizard_view_form" model="ir.ui.view">
        <field name="name">partner.matching.wizard.view.form</field>
        <field name="model">partner.matching.wizard</field>
        <field name="arch" type="xml">
            <form string="Partner Excel">
                <group>
                    <field name="contact_file" filename="file_name"/>
                    <field name="file_name" invisible="1"/>
                </group>
                <footer>
                    <button name="regenerate_partners" string="Submit"
                            type="object" class="btn-primary"/>
                    <button string="Discard" class="btn-secondary"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>