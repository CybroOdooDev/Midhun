<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_website_form" model="ir.ui.view">
        <field name="name">view.website.form.inherit.taxsurety</field>
        <field name="model">website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='language_ids']" position="after">
                <field name="is_shop_disabled"/>
                <field name="is_signup_disabled"/>
                <field name="is_customer_downpayment_enabled"/>
            </xpath>
        </field>
    </record>
</odoo>
